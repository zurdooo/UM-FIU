{
  "editor.tokenColorCustomizations": {
    "[*Light*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },
    "[*Dark*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#fff"
          }
        }
      ]
    }
  },
  "cSpell.words": [
    "Assignment",
    "buil",
    "Indexof",
    "perfectify",
    "Postfixer",
    "Prcd",
    "retval"
  ],
  "cSpell.ignoreWords": ["Lab"],
  "code-runner.executorMap": {
    "java": "cd $dir && javac -d ../out $fileName && java -cp ../out lab10.$fileNameWithoutExt"
  }
}
